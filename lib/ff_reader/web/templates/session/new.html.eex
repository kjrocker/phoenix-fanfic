<h1>Login</h1>
<%= form_for @conn, session_path(@conn, :create), [as: :session, class: "ui form"],
             fn f -> %>
  <div class="field">
  <%= text_input f, :email, placeholder: "Email" %>
  </div>
  <div class="field">
    <%= password_input f, :password, placeholder: "Password" %>
  </div>
  <%= submit "Login", class: "ui primary button fluid" %>
<% end %>
